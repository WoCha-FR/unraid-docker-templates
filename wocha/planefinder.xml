<?xml version="1.0"?>
<Container version="2">
  <Name>Planefinder Feeder</Name>
  <Repository>jeremiec82/planefinder</Repository>
  <Registry>https://hub.docker.com/r/jeremiec82/planefinder/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/Jeremie-C/docker-planefinder</Project>
  <Overview/>
  <Category>Other: Status:Beta</Category>
  <WebUI>http://[IP]:[PORT:30053]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/WoCha-FR/unraid-docker-templates/master/wocha/planefinder.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/WoCha-FR/unraid-docker-templates/master/wocha/imgs/planefinder.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>30053</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value/>
      <Name>BEAST_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>LATITUDE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>LONGITUDE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>SHARECODE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="BEAST_HOST" Target="BEAST_HOST" Default="" Mode="" Description="Conteneur Variable: BEAST_HOST" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="LATITUDE" Target="LATITUDE" Default="" Mode="" Description="Conteneur Variable: LATITUDE" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="LONGITUDE" Target="LONGITUDE" Default="" Mode="" Description="Conteneur Variable: LONGITUDE" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="SHARECODE" Target="SHARECODE" Default="" Mode="" Description="Conteneur Variable: SHARECODE" Type="Variable" Display="always" Required="false" Mask="false" />
  <Config Name="HTTP" Target="30053" Default="" Mode="tcp" Description="Conteneur Port: 30053" Type="Port" Display="advanced" Required="true" Mask="false" />
</Container>