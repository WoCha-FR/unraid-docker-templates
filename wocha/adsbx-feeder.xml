<?xml version="1.0"?>
<Container version="2">
  <Name>ADSB-Exchange Feeder</Name>
  <Repository>jeremiec82/adsbx</Repository>
  <Registry>https://hub.docker.com/r/jeremiec82/adsbx/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/Jeremie-C/docker-ADSBx</Project>
  <Overview/>
  <Category>Other: Status:Beta</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/WoCha-FR/unraid-docker-templates/master/wocha/adsbx-feeder.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/WoCha-FR/unraid-docker-templates/master/wocha/imgs/adsbx.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>30105</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>BEAST_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>ADSBX_UUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>LATITUDE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>LONGITUDE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>ALTITUDE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>MYUSERNAME</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="BEAST_HOST" Target="BEAST_HOST" Default="127.0.0.1" Mode="" Description="Conteneur Variable: BEAST_HOST" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="ADSBX_UUID" Target="ADSBX_UUID" Default="" Mode="" Description="Conteneur Variable: ADSBX_UUID" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="LATITUDE" Target="LATITUDE" Default="" Mode="" Description="Conteneur Variable: LATITUDE" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="LONGITUDE" Target="LONGITUDE" Default="" Mode="" Description="Conteneur Variable: LONGITUDE" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="ALTITUDE" Target="ALTITUDE" Default="" Mode="" Description="Conteneur Variable: ALTITUDE" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="MYUSERNAME" Target="MYUSERNAME" Default="" Mode="" Description="Conteneur Variable: MYUSERNAME" Type="Variable" Display="always" Required="true" Mask="false" />
  <Config Name="MLAT Results" Target="30105" Default="" Mode="tcp" Description="MLAT result input connextions" Type="Port" Display="advanced" Required="false" Mask="false" />
</Container>